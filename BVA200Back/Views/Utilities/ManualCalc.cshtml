                            </div>
                        </div>
                        <hr>
                        <div class="row sample-three mt-4">
                            <div class="col-1 d-flex align-items-center"><strong>3</strong></div>
                            <div class="col-3 d-flex align-items-center input-wrap position-relative">
                                <span class="micro-label">Hct</span>
                                @* hct *@
                                <input type="text"
