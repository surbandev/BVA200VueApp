                            </div>

                        </div>
                        <hr>
                        <div class="row sample-four mt-4">
                            <div class="col-1 d-flex align-items-center"><strong>4</strong></div>
                            <div class="col-3 d-flex align-items-center input-wrap position-relative">
                                <span class="micro-label">Hct</span>
                                @* hct *@
                                <input type="text"
